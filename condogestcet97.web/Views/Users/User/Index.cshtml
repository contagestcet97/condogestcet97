@model IEnumerable<condogestcet97.web.Data.Entities.Users.User>

@{
    ViewData["Title"] = "Users";
}

<h1>Users</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor((condogestcet97.web.Data.Entities.Users.User u) => u.Name)
            </th>
            <th>
                @Html.DisplayNameFor((condogestcet97.web.Data.Entities.Users.User u) => u.Email)
            </th>
            <th>
                @Html.DisplayNameFor((condogestcet97.web.Data.Entities.Users.User u) => u.PhoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor((condogestcet97.web.Data.Entities.Users.User u) => u.FiscalNumber)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model)
{
        <tr>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.PhoneNumber</td>
            <td>@item.FiscalNumber</td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
<a asp-action="Create" class="btn btn-primary">Create New</a>
